<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ResourceCompiler" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/resCompiler" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../../test" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-std=c99" />
				</Compiler>
				<Linker>
					<Add option="-lSDL" />
					<Add option="-lSDLmain" />
					<Add option="-lSDL_image" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="../build/bin/localsim/resCompiler" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-std=c99" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-lSDL" />
					<Add option="-lSDLmain" />
					<Add option="-lSDL_image" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="../../../resourceCompiler/jsmn.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../../../resourceCompiler/jsmn.h" />
		<Unit filename="../../../resourceCompiler/main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
